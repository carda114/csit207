<?php
    $myFirstVariable = 'Andrew';

    $literalString = 'My first variable was named $myFirstVariable.';
    echo $literalString;
    echo "\r\n";

    $dynamicString = "The value of my variable was $myFirstVariable.\r\n";
    echo $dynamicString;

    $empty = [];
    print_r($empty);

    $studentInfo = [
        'First Name' => 'Andrew',
        'Last Name' => 'Card',
        'Semester' => 'Fall 2022'
    ];
    print_r($studentInfo);

    $numArray = range(1, 50);
    print_r($numArray);

    $text = $literalString . $dynamicString;
    echo 'The number of characters in the $literalString and $dynamicString';
    echo ' variables are ' . strlen($text) . '.';
?>
